  <div>
    {{#dislocateParts forums}}
        <tr>
          <td class='forumName' id='{{this.forumId}}' data-urn = '{{this.forumURN}}' {{#if this.redactable}}data-redactable='true' data-deletable='true'{{/if}}>
            <a href="/{{this.forumURN}}" class="forumTitle">{{this.forumName}}</a> <br />
            {{this.comment}}
          </td>
          <td>{{this.sumTopics}}</td>
          <td>{{this.sumPosts}}</td>
          <td><a href="/{{this.forumURN}}/{{this.topicId}}?p={{ceil this.lastPage}}#{{this.lastPostId}}">смотреть</a> Автор: <a href = '/profile/{{this.userId}}'>{{this.userName}}</a> <br />{{getDate this.lastPostDate}}</td>
        </tr>
     {{/dislocateParts}}
